{
  "name": "dashboard-api",
  "version": "0.0.1",
  "description": "API serving the Connected Performance dashboard",
  "main": "build/src/index.js",
  "scripts": {
    "build": "rm -rdf build/ && tsc -p .",
    "dev": "nodemon --exec ts-node src/index.ts",
    "format": "prettier --ignore-path .gitignore --ignore-path .prettierignore --write \"**/*.+(js|ts|json)\"",
    "lint": "eslint \"src/**/*.{js,ts}\" --fix && tsc --noEmit",
    "start": "node ./build/src/index.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -d ./src/utils/connectToDb.ts"
  },
  "keywords": [],
  "author": "Connected Performance",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-rate-limit": "^6.0.0",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.11",
    "@types/nodemailer": "^6.4.7",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.22",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "aws-sdk": "^2.1381.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "file-type": "^18.5.0",
    "got": "^13.0.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1",
    "pg": "^8.10.0",
    "pino": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.14"
  }
}
